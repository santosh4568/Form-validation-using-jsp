<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Registration_Project</display-name>
  
  
  <servlet>
  	<servlet-name>loginPage</servlet-name>
  	<servlet-class>com.Registration.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginPage</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>SignupPage</servlet-name>
  	<servlet-class>com.Registration.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignupPage</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoadPage</servlet-name>
  	<servlet-class>com.Registration.LoadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoadPage</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
  
</web-app>